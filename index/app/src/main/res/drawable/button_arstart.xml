<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:startColor="#e420d817"
                android:centerColor="#dc048211"
                android:endColor="#e420d817"
                android:angle="270" />
            <stroke
                android:width="2dp"
                android:color="#1a7415" />
            <corners
                android:radius="100dp" />

        </shape>
    </item>
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#449def" />
            <stroke
                android:width="1dp"
                android:color="#2f6699" />
            <corners
                android:radius="2dp" />

        </shape>
    </item>
</selector>